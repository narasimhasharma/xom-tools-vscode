{
  "name": "sfiapex",
  "displayName": "SFI Apex Logs Analyser",
  "description": "VSCode extension for Salesforce Industries Apex Analysis",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.72.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:sfiapex.analyseOriginalSubmit",
    "onCommand:sfiapex.analyseSupplementalSubmit",
    "onCommand:sfiapex.analyseOrchestration",
    "onCommand:sfiapex.visualiseCpqLog",
    "onCommand:sfiapex.viewIntfImpls",
    "onCommand:sfiapex.viewSettingsToggles"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "sfiapex.analyseOriginalSubmit",
        "title": "OM Original Order Submit"
      },
      {
        "command": "sfiapex.analyseSupplementalSubmit",
        "title": "OM Supplemental Order Submit"
      },
      {
        "command": "sfiapex.analyseOrchestration",
        "title": "OM Orchestration"
      },
      {
        "command": "sfiapex.visualiseCpqLog",
        "title": "CPQ Visualise Log"
      },
      {
        "command": "sfiapex.viewIntfImpls",
        "title": "CPQ View Interface Impls"
      },
      {
        "command": "sfiapex.viewSettingsToggles",
        "title": "CPQ View Settings, Toggles"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "submenu": "sfiapex",
          "group": "navigation"
        }
      ],
      "sfiapex": [
        {
          "when": "!inOutput",
          "command": "sfiapex.analyseOriginalSubmit",
          "group": "navigation@1"
        },
        {
          "when": "!inOutput",
          "command": "sfiapex.analyseSupplementalSubmit",
          "group": "navigation@2"
        },
        {
          "when": "!inOutput",
          "command": "sfiapex.analyseOrchestration",
          "group": "navigation@3"
        },
        {
          "when": "!inOutput",
          "command": "sfiapex.visualiseCpqLog",
          "group": "navigation@4"
        },
        {
          "when": "!inOutput",
          "command": "sfiapex.viewIntfImpls",
          "group": "navigation@5"
        },
        {
          "when": "!inOutput",
          "command": "sfiapex.viewSettingsToggles",
          "group": "navigation@6"
        }
      ]
    },
    "submenus": [
      {
        "id": "sfiapex",
        "label": "SFI Apex"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.72.0",
    "@vscode/test-electron": "^2.0.3",
    "eslint": "^8.6.0",
    "glob": "^7.2.0",
    "mocha": "^9.1.3",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "node-fetch": "^2.6.1"
  }
}
